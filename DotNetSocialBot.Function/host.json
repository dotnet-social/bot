{
    "version": "2.0",
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "logLevel": {
          "default": "Information",
          "Host.Aggregator": "Trace",
          "Host.Results": "Information",
          "Function": "Information"
        },
        "enableLiveMetricsFilters": true
      }
    }
}